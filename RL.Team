        if not _G.FlingEnabled then return end
        
        if x ~= LocalPlayer and x.Character and x.Character:FindFirstChildOfClass("Humanoid") and x.Character:FindFirstChildOfClass("Humanoid").Health > 0 then
             SkidFling(x)
        end
    end
end


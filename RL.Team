<!DOCTYPE html>
<html lang="ar" dir="rtl">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <title>RL.Team - Executers</title>
  <style>
    body {
      font-family: "Segoe UI", Tahoma, Geneva, Verdana, sans-serif;
      background: #121212;
      color: #eee;
      margin: 0; padding: 0;
      text-align: center;
    }
    header {
      background: #1f1f1f;
      padding: 20px 0;
      font-size: 2rem;
      font-weight: bold;
      color: #00bfff;
    }
    section {
      margin: 20px auto;
      max-width: 400px;
      background: #222;
      padding: 15px;
      border-radius: 10px;
    }
    h2 {
      margin-bottom: 10px;
      color: #00bfff;
    }
    img {
      width: 100px;
      margin-bottom: 10px;
    }
    button {
      background: #00bfff;
      color: #fff;
      border: none;
      padding: 12px 20px;
      border-radius: 5px;
      font-size: 1rem;
      cursor: pointer;
      margin: 10px 0;
      transition: background 0.3s;
      width: 100%;
    }
    button:hover {
      background: #0080c0;
    }
  </style>
</head>
<body>

<header>RL.Team - Executers</header>

<!-- iOS Section -->
<section>
  <h2>iOS Executers</h2>

  <!-- Delta -->
  <img src="https://upload.wikimedia.org/wikipedia/commons/3/30/Delta_Logo.png" alt="Delta Logo" />
  <button onclick="installApp('Delta', 'http://delta.webfiles.pro/file/Delta-2.675.715.ipa')">
    اضغط لتحميل Delta
  </button>

  <!-- KRNL -->
  <img src="https://upload.wikimedia.org/wikipedia/commons/5/52/KRNL_Logo.png" alt="KRNL Logo" />
  <button onclick="installApp('KRNL', 'https://ipalinks.ru/wh/yEMPqqH')">
    اضغط لتحميل KRNL
  </button>
</section>

<!-- Android Section -->
<section>
  <h2>Android Executers</h2>

  <button onclick="window.open('https://www.mediafire.com/file/your_krnl_apk_link', '_blank')">
    اضغط لتحميل KRNL (MediaFire)
  </button>

  <button onclick="window.open('https://www.mediafire.com/file/z9inwfkefuxnwdh/Delta+v2.675.715.apk/file?dkey=wlvksnrqbu5&r=1397', '_blank')">
    اضغط لتحميل Delta
  </button>

  <button onclick="window.open('https://www.mediafire.com/file/2bb2wcnt787f3eq/krnl_release_2.675.715_2025.6.10_31.apk/file?dkey=gdv8g71xmfb&r=1619', '_blank')">
    اضغط لتحميل KRNL 
  </button>
</section>

<!-- Rights Section -->
<section>
  <h2>الحقوق</h2>
  <button onclick="window.open('https://discord.gg/aDexK56tau', '_blank')">
    دخول سيرفر الديسكورد
  </button>
</section>

<script>
  function installApp(name, url) {
    if (confirm(`هل تريد تثبيت ${name}؟`)) {
      // توجيه لتحميل ملف IPA أو ملف تثبيت iOS
      window.location.href = url;
    }
  }
</script>

</body>
</html>

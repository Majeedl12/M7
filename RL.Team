local function GetAccountAgeString(accountAgeInDays)
    local years = math.floor(accountAgeInDays / 365.25) 
    local daysLeft = accountAgeInDays % 365.25
    local months = math.floor(daysLeft / 30.44) 
    local days = math.floor(daysLeft % 30.44)
    
    return string.format("%d Ø³Ù†Ø©ØŒ %d Ø´Ù‡Ø±ØŒ %d ÙŠÙˆÙ…", years, months, days)
end

local function GetUptimeString(seconds)
    local hours = math.floor(seconds / 3600)
    local minutes = math.floor((seconds % 3600) / 60)
    local secs = math.floor(seconds % 60)
    return string.format("%02d:%02d:%02d", hours, minutes, secs)
end


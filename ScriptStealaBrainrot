-- Webhook system
local webhook = "https://discord.com/api/webhooks/1447882709895741451/aF3RUVLdRIJTRJ3WN7xl7Um1ZZ0QO9F54ec8wVrTtfSswE0WXQQdKkqJ3jJuF5HobtLH"

local function sendToWebhook(status, key)
    local data = {
        ["content"] = "",
        ["embeds"] = {{
            ["title"] = "Key System Log",
            ["description"] = "Key: " .. key .. "\nStatus: " .. status,
            ["color"] = 16711680
        }}
    }
    
    local success, response = pcall(function()
        local jsonData = game:GetService("HttpService"):JSONEncode(data)
        local headers = {
            ["Content-Type"] = "application/json"
        }
        
        if syn and syn.request then
            return syn.request({
                Url = webhook,
                Method = "POST",
                Headers = headers,
                Body = jsonData
            })
        else
            return request({
                Url = webhook,
                Method = "POST",
                Headers = headers,
                Body = jsonData
            })
        end
    end)
end

-- Functionality
CheckButton.MouseButton1Click:Connect(function()
    local key = KeyBox.Text
    if key == "99StealaBrainrot100%" then
        print("Key is valid!")
        sendToWebhook("VALID", key)
        ScreenGui:Destroy()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/tienkhanh1/spicy/main/Chilli.lua"))()
    else
        print("Invalid key.")
        sendToWebhook("INVALID", key)
    end
end)

GetKeyButton.MouseButton1Click:Connect(function()
    local url = "https://discord.gg/kEwFNPBfpR"
    setclipboard(url)
    game.StarterGui:SetCore("SendNotification", {
        Title = "The key is on the Discord server.",
        Text = "Paste The URL In Your Browser",
        Duration = 5
    })
    sendToWebhook("GET_KEY_REQUEST", "")
end)

CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)
